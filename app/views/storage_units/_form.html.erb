<div class="form-row"><br>
  <%= f.label :name %>
  <%= f.text_field :name, placeholder: 'Name', class: 'form-control' %><br><br><br>
</div>
<div class="form-row">
  <%= f.label :about %>
  <%= f.text_field :about, placeholder: 'About', class: 'form-control' %><br><br><br>
</div>
<div class="form-row">
  <%= f.label :contact_no %>
  <%= f.text_field :contact_no, placeholder: 'Contact No', class: 'form-control' %><br><br><br>
</div>

<div class="form-row">
  <div class="col-lg-12">
    <label>Address:</label>
  </div>
</div>
<div class="form-row">
  <div class="col-lg-12">
    <div id="storageUnitAddress">
      <%= f.fields_for :address do |address| %>
        <%= render 'address_fields', f: address %>
      <% end %>
    </div>
  </div>
</div>

<%= render partial: 'shared/provided_amenities', locals: {f: f} %><br>