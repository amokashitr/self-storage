<div class="container">
  <br><br><h1>New Unit</h1>

  <%= form_for [@storage_unit,@unit] do |f| %>
    <%= render partial: 'form', locals: {f: f} %>

    <div class="form-row">
      <%= f.label :images %>&emsp;
      <%= f.file_field :images, multiple: true %><br><br><br>
    </div>

    <div class="form-row">
      <button type="submit" class="btn btn-primary">Save</button><br>
    </div><br><br>
  <% end %>

</div>